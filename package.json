{
	"name": "remotion-template",
	"version": "1.0.0",
	"description": "My Remotion video",
	"scripts": {
		"start": "remotion preview src/index.tsx",
		"build": "remotion render src/index.tsx Main out.mp4",
		"upgrade": "remotion upgrade",
		"render-write-in-react": "remotion still src/index.tsx WriteInReactHomepage --scale=0.5 out/writeinreact.png",
		"render-player": "remotion still src/index.tsx PlayerExample --frame=167 out/player-example.png",
		"render-timeline-split": "remotion render src/index.tsx TimelineSplit --frames=0-230 out/timelinesplit.mp4 --scale=0.5",
		"render-npminstall": "remotion render src/index.tsx NpmInstallVideoLight --frames=0-230 out/npminstalllight.mp4 --scale=0.5 && remotion render src/index.tsx NpmInstallVideoDark --frames=0-230 out/npminstalldark.mp4 --scale=0.5",
		"render-cluster": "remotion still src/index.tsx Cluster --frame=157 out/cluster.png",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc"
	},
	"repository": {},
	"license": "UNLICENSED",
	"dependencies": {
		"@remotion/cli": "3.0.0-lambda.457+38f94c0c6",
		"@remotion/eslint-config": "3.0.0-lambda.457+38f94c0c6",
		"@remotion/lambda": "3.0.0-lambda.457+38f94c0c6",
		"@types/opentype.js": "^1.3.3",
		"@types/react": "18.0.1",
		"@types/web": "^0.0.21",
		"eslint": "8.13.0",
		"opentype.js": "^1.3.4",
		"polished": "^4.1.4",
		"prettier": "^2.2.1",
		"prettier-plugin-organize-imports": "^1.1.1",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"remotion": "3.0.0-lambda.457+38f94c0c6",
		"simplex-noise": "^2.4.0",
		"svg-path-properties": "^1.0.11",
		"ts-node": "^9.1.1",
		"typescript": "4.6.3"
	}
}
